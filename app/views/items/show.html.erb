<div class="jumbotron tag-format">
  <div class=row>
    <h2><%= @item.name %></h2>
    <div><%= @item.description %></div>
      <%= link_to 'Edit', edit_item_path(@item),:class=> 'btn btn-primary'%>
      <%= link_to 'Delete', item_path(@item), 
      method: :delete, 
      data: {confirm: "Are you sure ?"},
      :class=> 'btn btn-danger' %>
  </div>
</div>
